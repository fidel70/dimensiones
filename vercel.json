{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "continue": true,
      "headers": {
        "WWW-Authenticate": "Basic realm=\"Secure Area\"",
        "Access-Control-Allow-Origin": "*"
      },
      "middleware": {
        "auth": "Basic ${process.env.AUTH_HEADER}"
      }
    }
  ]
}
